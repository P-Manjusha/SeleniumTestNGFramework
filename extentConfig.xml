<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>standard</theme>
    
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>
        
        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>http</protocol>
        
        <!-- title of the document -->
        <documentTitle>AmazonTestingProject</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        <reportName>Amazon Project Test  Report
         <![CDATA[
                <img src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSMTJvT6FXTPy3L5IpqooHsLamhuXCnYadpLFOww_ZR66xLWoHEszid7_w&s'/>
            ]]>
        </reportName>
        
        
        <!-- location of charts in the test view -->
        <!-- top, bottom -->
        <testViewChartLocation>bottom</testViewChartLocation>
      
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
        </scripts>
        
        <!-- custom styles -->
        <styles>
            <![CDATA[
            .report-name { padding-left: 18px; } .report-name > img { float: 
      left;height: 90%;margin-left: 30px;margin-top: 3px;width: auto; }
        ]]>
        </styles>
    </configuration>
</extentreports>